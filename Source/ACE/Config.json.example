{
  "Server": {
    "WorldName": "ACEmulator",
    "Welcome": "Welcome to this ACE server!\nFor more information visit http://www.acemulator.org.",
    "Network": {
      "Host": "0.0.0.0",
      "Port": 9000,
      "MaximumAllowedSessions": 128,
      "DefaultSessionTimeout": 60
    },
    "Accounts": {
      "OverrideCharacterPermissions": true,
      "DefaultAccessLevel": 0
    },
    "DatFilesDirectory": "c:\\ACE\\",
    "ShutdownInterval": "60",
    "SecureAuthentication": false,
    "AllowedAuthServers": [
      "http://localhost:8001",
      "http://acemulator.org"
    ]
  },
  "ApiServer": {
    "ListenUrl": "http://*:8000",
    "PublicUrl": "http://localhost:8000"
  },
  "AuthServer": {
    "ListenUrl": "http://*:8001",
    "PublicUrl": "http://localhost:8001"
  },
  "ContentServer": {
    "LocalContentPath": "Content\\"
  },
  "MySql": {
    "Authentication": {
      "Host": "127.0.0.1",
      "Port": 3306,
      "Database": "ace_auth",
      "Username": "root",
      "Password": ""
    },
    "Shard": {
      "Host": "127.0.0.1",
      "Port": 3306,
      "Database": "ace_shard",
      "Username": "root",
      "Password": ""
    },
    "World": {
      "Host": "127.0.0.1",
      "Port": 3306,
      "Database": "ace_world",
      "Username": "root",
      "Password": ""
    }
  }
}
